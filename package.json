  "dependencies": {
    "@turf/turf": "^7.2.0",
    "@turf/union": "^7.2.0",
    "@types/sqlite3": "^3.1.11",
    "axios": "^1.7.7",
    "better-sqlite3": "^11.8.1",
    "cheerio": "^1.0.0",
    "client-nmea-socket-to-event": "^1.1.6",
    "colors": "^1.4.0",
    "crc-32": "^1.2.2",
    "express": "^4.21.1",
    "https-proxy-agent": "^7.0.5",
    "ink": "^4.3.1",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "nmea-simple": "^3.3.0",
    "serialport": "^12.0.0",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "socketstream2event": "^1.1.0",
    "socks-proxy-agent": "^8.0.4",
    "sqlite3": "^5.1.7"
